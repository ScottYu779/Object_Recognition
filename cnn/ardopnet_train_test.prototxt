}
layer {
name: "flatdata"
type: "Flatten"
bottom: "pool3"
top: "flatdata"
}
layer {
  name: "drop4"
  type: "Dropout"
  bottom: "flatdata"
  top: "flatdata"
  dropout_param {
    dropout_ratio: 0.2
  }
}
layer {
  name: "ip1"
